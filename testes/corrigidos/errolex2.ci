@cep(10, "usando 'this' no lugar de 'self'", "token 'this' not recognized")

class Teste
    var Int n;

    func get -> Int {
        return self.n;
    }

    func set: Int n {
        this.n = n;
    }
end

class Program
    func run {
        var Teste t;
        var Int a;
        t = Teste.new;
        a = In.readInt;
        t.set: a;
        Out.println: t.get;
    }
end
